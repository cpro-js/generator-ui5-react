    "@sap-ui/common-css": "^0.38.0",
    "@ui5/webcomponents": "~1.23.3",
    "@ui5/webcomponents-fiori": "~1.23.3",
    "@ui5/webcomponents-react": "~1.26.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal-promise": "^1.0.2",
